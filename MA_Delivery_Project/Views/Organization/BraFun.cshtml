@model MA_Delivery_Project.DBConnStr.AllMod

@{
	ViewBag.Title = "Branches";

}

@{
	<div class="form-row">
			<div class="form-group col-md-1">

				<label> Code</label>
				<input type="text" class="form-control" id="txtcode" placeholder=" Code">
			</div>
			<div class="form-group col-md-2">

				<label> Select Organize </label>
            <select class="form-control select2-1" id="SelIDv" >
				@foreach(var val in Model.OrgsSubList)
			     {
					 <option value="@val.ID">@val.OrgsSubName </option>
				 }
				                            
				
			 	
				</select>
			</div>
	</div>
	<div class="form-row">

			<div class="form-group col-md-3">

				<label>Branch Name</label>
				<input type="text" class="form-control" id="txtName" placeholder="Name">
			</div>
			<div class="form-group col-md-3">

				<label>Latitude</label>
				<input type="text" class="form-control" id="txtLatitude" placeholder="Latitude">
			</div>
			<div class="form-group col-md-3">

				<label>Longtitude</label>
				<input type="text" class="form-control" id="txtlongtitude" placeholder="Longtitude">
			</div>

	</div>
	<div class="form-row">
			<div class="form-group col-md-3">
			<input type="button" value="Save" class="btn btn-success" id="BtnSaveBr" >

			</div>
	</div>

}
