@model MA_Delivery_Project.DBConnStr.AllMod
@{
	ViewBag.Title = "Contract All";
}
@{
	<div class="form-row">
		<div class="form-group col-md-3">

					<label> Select Organization</label>
					<select class="form-control" id="select2-1" onchange="FT();">
			 	 
					@foreach(var Modelitem in Model.OrgsSubList)
				    {
					<option value="@Modelitem.ID">@Modelitem.OrgsSubName</option>
			 	    }
					</select>

				</div>
	</div>
	<div class="form-row">
				<div class="form-group col-md-1">

					<label> Code</label>
					<input type="text" class="form-control" id="txtContractCode" placeholder=" Code">
				</div>
				<div class="form-group col-md-3">

					<label> Name</label>
					<input type="text" class="form-control" id="txtOwnerName" placeholder=" Name">
				</div>
				<div class="form-group col-md-3">

					<label> Address</label>
					<input type="text" class="form-control" id="txtAddress" placeholder=" Address">
				</div>
				<div class="form-group col-md-3">

					<label> Phone1</label>
					<input type="text" class="form-control" id="txtphone1" placeholder=" Phone1">
				</div>
				<div class="form-group col-md-3">

					<label> Phone2</label>
					<input type="text" class="form-control" id="txtphone2" placeholder=" Phone2">
				</div>
				<div class="form-group col-sm-5 col-lg-4">
					<!-- BEGIN Input Group -->
					<label> Date Range</label>
					<div class="input-group input-daterange">
						<input type="text" class="form-control" placeholder="From" da>
						<div class="input-group-prepend input-group-append">
							<span class="input-group-text">
								<i class="fa fa-ellipsis-h"></i>
							</span>
						</div>
						<input type="text" class="form-control" placeholder="To">
					</div>

				</div>
				<div class="form-group col-md-3">

					<label> Contact Cost</label>
					<input type="text" class="form-control" id="txtCost" placeholder="Contact  Cost">
				</div>

	</div>

}
